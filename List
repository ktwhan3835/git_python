#List of python
a = [1.0, 2.0, 3.0]
a.append(4.0)
print(a)

a.insert(0,0.0)
print(a)

print(len(a))

a[2:4] = [1.0, 1.0, 1.0] #2,3은 대체후 끼워넣기로 하나 추가됨
print(a)

s = [1.0, 2.0, 3.0]
print(s)

b = s
print(b)

print(b[0])
b[0] = 5.0

print(b)
print(s)
print(b[0])
print(b[1])

c = s[:]
c[0] = 1.0
print(s)
print(c)
